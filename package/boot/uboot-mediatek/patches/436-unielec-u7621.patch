+bootmenu_4=Reset all settings to factory defaults.=run reset_factory ; reset
+boot_default=led system on; sleep 1; button reset && run boot_dhcptftp_forever; led wlan5g off; led system off; run boot_production
+boot_production=led system on; run nor_read_production && led system off && bootm $loadaddr#$bootconf; led system on
+boot_dhcptftp=dhcp && bootm
+boot_dhcptftp_forever=while true ; do led wlan5g on; run boot_dhcptftp ; led wlan5g off; sleep 1 ; done
+nor_read_production=sf probe && sf read $loadaddr 0x90000 0x400000
+reset_factory=env default -f -a && env save && reset
+_init_env=env set _init_env; run _env_set_ethaddr; env save
+_env_set_ethaddr=env set _env_set_ethaddr; sf probe && sf read $loadaddr 0x5e000 0x6 && env set -f ethaddr && env readmem -b ethaddr $loadaddr 0x6
+_firstboot=env set _firstboot; run _switch_to_menu; run _init_env; bootmenu
+_switch_to_menu=env set _switch_to_menu; env set bootdelay 3; env set bootmenu_delay 3; env set bootmenu_0 $bootmenu_0d; env set bootmenu_0d; run _bootmenu_update_title
+_bootmenu_update_title=env set _bootmenu_update_title; env set bootmenu_title "$bootmenu_title       [33m$ver[0m"
